.slick-dots
{

	margin-top: 20px;

    display: block;

    width: 100%;
    padding: 0;


    list-style: none;

    text-align: center;
}


.slick-dots li
{
    position: relative;

    display: inline-block;

    width: 20px;
    height: 20px;
    margin: 0 5px;
    padding: 0;

    cursor: pointer;
}


.slick-dots li button
{
    font-size: 0;
    line-height: 0;

    display: block;

    width: 20px;
    height: 20px;
    padding: 5px;

    cursor: pointer;

    color: transparent;
    border: 0;
    outline: none;
    background: transparent;
    -webkit-appearance: none;
	border-radius: 0;
}

.slick-dots li button:hover,
.slick-dots li button:focus
{
    outline: none;
}

.slick-dots li button:hover:before,
.slick-dots li button:focus:before
{
    opacity: 1;
}

.slick-dots li button:before
{
    font-size: 0px;
    line-height: 20px;

    position: absolute;
    top: 5px;
    left: 5px;

    width: 10px;
    height: 10px;

    content: '•';
    opacity: .25;

    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;

    border-radius: 50%;
    background-color: #444;
}
.slick-dots li.slick-active button:before
{
    opacity: .75;
}





